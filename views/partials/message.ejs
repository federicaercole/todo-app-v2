<% if (success && success.length>0) { %>
    <div class="info success" aria-live="polite">
        <p>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" aria-hidden="true" focusable="false">
                <path fill="currentColor" d="m9 20.42l-6.21-6.21l2.83-2.83L9 14.77l9.88-9.89l2.83 2.83L9 20.42Z" />
            </svg>
            <%= success %>
        </p>
        <button type="button" class="close">
            <span class="visually-hidden">Close message</span><svg xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 24 24" aria-hidden="true" focusable="false">
                <path fill="currentColor"
                    d="M19 6.41L17.59 5L12 10.59L6.41 5L5 6.41L10.59 12L5 17.59L6.41 19L12 13.41L17.59 19L19 17.59L13.41 12z" />
            </svg></button>
    </div>
    <% }%>
        <% if (warning && warning.length>0) { %>
            <div class="info warning" aria-live="polite">
                <p>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" aria-hidden="true" focusable="false">
                        <path fill="currentColor" d="M12 5.99L19.53 19H4.47L12 5.99M12 2L1 21h22L12 2z" />
                        <path fill="currentColor" d="M13 16h-2v2h2zm0-6h-2v5h2z" />
                    </svg>
                    <%= warning %>
                </p>
                <button type="button" class="close">
                    <span class="visually-hidden">Close message</span><svg xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 24 24" aria-hidden="true" focusable="false">
                        <path fill="currentColor"
                            d="M19 6.41L17.59 5L12 10.59L6.41 5L5 6.41L10.59 12L5 17.59L6.41 19L12 13.41L17.59 19L19 17.59L13.41 12z" />
                    </svg></button>
            </div>
            <% }%>
                <% if (error && error.length>0) { %>
                    <div class="info error" aria-live="polite">
                        <div>
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" aria-hidden="true"
                                focusable="false">
                                <path fill="currentColor"
                                    d="M11 15h2v2h-2zm0-8h2v6h-2zm.99-5C6.47 2 2 6.48 2 12s4.47 10 9.99 10C17.52 22 22 17.52 22 12S17.52 2 11.99 2zM12 20c-4.42 0-8-3.58-8-8s3.58-8 8-8s8 3.58 8 8s-3.58 8-8 8z" />
                            </svg>
                            <span class="visually-hidden">There was a problem</span>
                            <ul>
                                <% error.forEach(item=>{%>
                                    <li>
                                        <%= item.msg %>
                                    </li>
                                    <% }) %>
                            </ul>
                        </div>
                        <button type="button" class="close">
                            <span class="visually-hidden">Close message</span><svg xmlns="http://www.w3.org/2000/svg"
                                viewBox="0 0 24 24" aria-hidden="true" focusable="false">
                                <path fill="currentColor"
                                    d="M19 6.41L17.59 5L12 10.59L6.41 5L5 6.41L10.59 12L5 17.59L6.41 19L12 13.41L17.59 19L19 17.59L13.41 12z" />
                            </svg></button>
                    </div>
                    <% }%>